	email = rashmiranjan.rout@us.fujitsu.com
	name = Rashmi Ranjan Rout
[color]
	ui = auto
	interactive = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = red
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
	log = true
[difftool]
	prompt = true
[core]
	editor = vim
[push]
	default = simple
